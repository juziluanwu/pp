    <select id="selectAll" parameterType="map" resultType="app.pp.entity.SaleVoidRecord">
        select ss.*,d.devicenum,p.pnum,su.name creatorname,g.name firstbeneficiaryname
        from salevoidrecord ss
        left join device d on ss.deviceid = d.id
        left join policy p on p.id = ss.policyid
        left join sys_user su on ss.creator = su.name
        left join group g on g.id = ss.firstbeneficiary
        <where>
            <if test="devicenum != null and devicenum != ''">
                and d.devicenum like '%${devicenum}%'
            </if>
            <if test="pnum != null and pnum != ''">
                and p.pnum like '%${pnum}%'
            </if>
            <if test="customername != null and customername != ''">
                and ss.customername like '%${customername}%'
            </if>
            <if test="username != null and username != ''">
                and su.name like '%${username}%'
            </if>
            <if test="firstbeneficiary != null">
                and ss.firstbeneficiary = #{firstbeneficiary}
            </if>
            <if test="policystate != null">
                and ss.policystate = #{policystate}
            </if>

               <if test="pstarttime != null">
                and ss.pstarttime  <![CDATA[ >= ]]> #{pstarttime}
              </if>
              <if test="pendtime != null">
                and ss.pendtime <![CDATA[ <= ]]> #{pendtime}
              </if>

            <if test="groups != null">
                and ss.groupid in
                <foreach collection="groups" open="(" close=")" separator="," item="group">
                    #{group.id}
                </foreach>
            </if>
        </where>
        order by ss.id desc
    </select>

    <select id="selectById" parameterType="Integer" resultType="app.pp.entity.SaleVoidRecord">
